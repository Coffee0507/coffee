package com.internousdev.Action;

import org.springframework.web.bind.annotation.RestController;

import com.internousdev.Interface.AbcInterface;

@RestController
public class AbcAction {

	AbcInterface abc;


	@RequestMapping("/yyy")
	public void action() {
		abc.getJog();
		abc.getRun();
		abc.getWalk();

	}

}
