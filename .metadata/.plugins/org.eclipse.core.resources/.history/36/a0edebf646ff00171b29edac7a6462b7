package Service;

import Interface.AbcInterface;

public class AbcService implements AbcInterface{

	@Override
	public void getRun() {
		String msg = "走る";
		System.out.println(msg);
		return msg;

	}

	@Override
	public String getJog() {
		String msg = "ジョギング";
		System.out.println(msg);
		return msg;
	}

	@Override
	public String getWalk() {
		String msg = "歩く";
		System.out.println(msg);
		return msg;
	}

}
