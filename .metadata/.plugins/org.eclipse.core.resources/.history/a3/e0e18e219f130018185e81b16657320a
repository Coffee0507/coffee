package Practice;

import java.util.ArrayList;
import java.util.Collections;

public class Collection2 {

	public static void main(String[] args){

		int[] n = {1,2,3,4,5};

		ArrayList<Integer> N = new ArrayList<>();

		for(int N1 : n){
			N.add(N1);
		}

		Collections.sort(N,(a,b) ->{
			return a+b;
		});

		for(Integer N2 : N){
			System.out.println(N);
		}
	}

}
