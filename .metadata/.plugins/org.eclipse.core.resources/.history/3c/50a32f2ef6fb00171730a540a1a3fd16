package object;

// interfaceを実装
public class Human implements HumanInterface {

	private String run;
	private String eat;
	private String sleep;

	@Override
	public void getRun() {
		this.run = run;
	}

	@Override
	public void getEat() {
		this.eat = eat;
	}

	@Override
	public void getSleep() {
		this.sleep = sleep;
	}

}
