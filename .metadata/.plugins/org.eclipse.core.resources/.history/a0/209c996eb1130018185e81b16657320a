package Practice;

import java.util.ArrayList;
import java.util.Collections;

public class Collection2 {

	public static void main(String[] args){

		int[] n = {1,2,3,4,5};

		ArrayList<Integer> N = new ArrayList<>();

		for(Integer N1 : n){
			System.out.println(N1 + " " );
		}

		Collections.sort(N1,(a,b) ->{
			return a-b;
		});

	}

}
