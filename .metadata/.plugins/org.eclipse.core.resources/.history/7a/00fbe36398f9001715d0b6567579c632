<struts>
	<constant name="struts.devMode" value="true" />

	<package name="com.Action" extends="struts-default">


		<action name="AcountConfirm" class="com.Action.AcountConfirm" method="execute">
			<result name="success">acountconfirm.jsp</result>
			<result name="error">acountinput.jsp</result>
		</action>

		<action name="NumberInputAction" class="com.Action.NumberInputAction" method="execute">
			<result name="success">numberinput.jsp</result>
		</action>

		<action name="Calculator" class="com.Action.Calculator" method="execute">
			<result name="success">result.jsp</result>
			<result name="error">acountinput.jsp</result>
		</action>

	</package>
