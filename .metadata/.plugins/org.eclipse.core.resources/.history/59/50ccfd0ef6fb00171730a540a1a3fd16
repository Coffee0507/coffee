package object;

// interfaceを実装
public class Human implements HumanInterface {

	private String run;
	private String eat;
	private String sleep;

	@Override
	public void run() {
		this.run = run;
	}

	@Override
	public void eat() {
		this.eat = eat;
	}

	@Override
	public void sleep() {
		this.sleep = sleep;
	}

}
