package com.example.demo;

import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

@RestController
public class demoController {

	@Autowired
	demoInterface fa;

	@RequestMapping("/b")
	public void action(){
	fa.getRun(msg) {
		return msg;
	}

	public String getWalk(String msg){
		return msg;
	}

	public String getJog(String msg) {
		return msg;
	}
	}
}
