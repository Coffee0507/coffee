package com.example.demo;

import org.springframework.stereotype.Service;

@Service
public class demoService implements demoInterface {


	@Override
	public void getRun() {
		String msg = "走る";
		System.out.println(msg);
	}

	@Override
	public void getWalk() {
		String msg = "歩く";
		System.out.println(msg);

	}

	@Override
	public void getJog(String msg) {
		msg = "ジョギング";
		this.msg = msg;
		System.out.println(msg);

	}

}
