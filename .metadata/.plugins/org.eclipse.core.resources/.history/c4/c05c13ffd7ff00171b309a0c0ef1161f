package com.example.demo;

import org.springframework.stereotype.Service;

@Service
public class demoService implements demoInterface {

	String msg:

	public void getRun(String msg) {
		String msg = "走る";
		System.out.println(msg);
	}

	public void getWalk() {
		String msg = "歩く";
		System.out.println(msg);

	}

	public void getJog() {
		String msg = "ジョギング";
		System.out.println(msg);

	}

}
