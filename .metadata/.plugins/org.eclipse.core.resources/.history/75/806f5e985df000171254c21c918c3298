package com.internousdev.sample.DAO;

import java.sql.Connection;
import java.sql.PreparedStatement;
import java.sql.ResultSet;

import com.internousdev.sample.DTO.LoginDTO;
import com.internousdev.sample.util.DBConnector;

public class LoginDAO {
	private DBConnector db = new DBConnector();
	private Connection con = db.getConnection();
	private LoginDTO loginDTO = new LoginDTO();
	private String sql = "select*from login_user_transaction";

	public LoginDTO loginUserInfo(String loginUserId, String loginPass) {

		try{
			PreparedStatement ps = con.prepareStatement(sql);

			ps.setString(1, "loginUserId");
			ps.setString(2, "loginPass");

			ResultSet rs = ps.executeQuery();

			if(!(rs.getString("loginId").equals(null))){
				if(!(rs.getString("login_pass").equals(null))){




				}
			}




		}catch(Exception e){
			e.printStackTrace();
		}





		return null;
	}

}
