<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<script type="text/javascript">
	function putternA() {
		request.setAttribute("log", "A");
	}
</script>

<title>インプット画面</title>
</head>
<body>

	<s:form action="InputAction">
		<s:submit value="パターンA" onClick="putternA()">request.setAttribute("A","A")</s:submit>
		<s:submit value="パターンB">
			<s:param name="log" value="patternB" />
		</s:submit>
		<s:submit value="パターンC">
			<s:param name="log" value="patternC" />
		</s:submit>
	</s:form>
